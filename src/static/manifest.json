{
  "manifest_version": 3,
  "name": "pocket-gpt",
  "description": "pocket-gpt chrome extension",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "PocketGPT",
    "default_icon": "icon.png"
  },
  "permissions": ["tabs"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  }
}
